    <section class="o-hidden">
        <div class="container">
            <div
                class="pb-5 row align-items-center justify-content-center justify-content-lg-between text-center text-lg-left flex-lg-row-reverse">
                <div class="col-md-9 col-lg-6 col-xl-5 mb-4 mb-lg-0 pr-lg-5 pr-xl-0">
                    <div data-aos="fade-in" data-aos-delay="250">
                        <h1 id="claim" class="display-3">{{sec.top.title.[0]}} <mark data-aos="highlight-text"
                                data-aos-delay="500">{{sec.top.title.[1]}}</mark><br />{{sec.top.title.[2]}}
                            <mark data-aos="highlight-text" data-aos-delay="750">{{sec.top.title.[3]}}.</mark></h1>
                        <p class="lead">{{sec.top.subtitle}}</p>
                        <div class="mt-4 mt-md-5">
                            <a href="#boost" class="btn-twip btn btn-primary flex-shrink-0">
                                <span>{{sec.top.cta}}</span>
                            </a>
                        </div>
                    </div>
                </div>
                <div class="col-md-9 col-lg-6" data-aos="fade-left">
                    <div id="twipLogoContainer" class="d-none d-lg-block">
