                        <img id="yellowBg" width="100%" src="/assets/img/twipLogo_yellowBg.png" alt="yellowBg" />
                        <img id="outline" width="100%" src="/assets/img/twipLogo_outline.png" alt="outline" />
                        <img id="font" width="100%" src="/assets/img/twipLogo_font.png" alt="font" />
                    </div>
                </div>
            </div>
        </div>
    </section>
    <section id="boost" class="pb-0 pt-0">
        <div class="divider divider-bottom bg-primary-3 mt-5"></div>
    </section>
    <section class="pt-5 bg-primary-3 text-black">
        <div class="container">
            <div class="row section-title justify-content-center text-center">
                <div class="col-md-10 col-lg-9 col-xl-8">
                    <h3 class="display-4">{{sec.boost.offering.title}}</h3>
                    <div class="lead">{{sec.boost.offering.subtitle}}</div>
                </div>
            </div>
            <div class="row align-items-center justify-content-around text-center text-lg-left pt-0">
                <div class="col-md-9 col-lg-6 col-xl-5 mb-4 mb-md-5 mb-lg-0 pl-lg-5 pl-xl-0">
                    <div>
                        <h6>> {{sec.boost.offering.startup.as}}</h6>
                        <h3 class="h1">{{sec.boost.offering.startup.title}}</h3>
                        <p class="lead">{{sec.boost.offering.startup.text}}</p>
                        <a href={{sec.boost.offering.startup.mailTemplate}} class="btn-twip btn-twip-alt btn btn-primary flex-shrink-0">
                            <span>{{sec.boost.offering.startup.cta}}</span>
                        </a>
                    </div>
                </div>
                <div class="col-md-9 col-lg-6 col-xl-5 p-4">
                    <img src="/assets/img/illustration_startup.png" alt="Startup Illustration" class="img-fluid">
                </div>
            </div>
            <div class="row align-items-center justify-content-around text-center text-lg-left pt-5">
                <div class="col-md-9 col-lg-6 col-xl-5 mb-4 mb-md-5 mb-lg-0 order-lg-2 pl-lg-5 pl-xl-0">
                    <div>
                        <h6>> {{sec.boost.offering.corporate.as}}</h6>
                        <h2 class="h1">{{sec.boost.offering.corporate.title}}</h2>
                        <p class="lead">{{sec.boost.offering.corporate.text}}</p>
                        <a href={{sec.boost.offering.corporate.mailTemplate}} class="btn-twip btn btn-primary flex-shrink-0">
                            <span>{{sec.boost.offering.corporate.cta}}</span>
                        </a>
                    </div>
                </div>
                <div class="col-md-9 col-lg-6 col-xl-5 order-lg-1 p-4">
                    <img src="/assets/img/illustration_corporate.png" alt="Illustration Corporate" class="img-fluid">
                </div>
            </div>
        </div>
    </section>

    <section class="pb-5">
        <div class="container">
            <div class="row section-title justify-content-center text-center">
                <div class="col-md-10 col-lg-9 col-xl-8">
                    <h3 class="display-4">{{sec.boost.valueProp.title}}</h3>
